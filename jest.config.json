{
  "name": "markdown-decorator",
  "automock": false,
  "collectCoverage": true,
  "coverageDirectory": "./coverage",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/.assets/scripts/markdown-decorator/__tests__/",
    "/__tests__/__fixtures__/",
    "/__fixtures__/",
    "/__mocks__/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "testPathIgnorePatterns": [
    "/__tests__/__fixtures__/",
    "/__fixtures__/",
    "/__mocks__/"
  ],
  "verbose": true
}
